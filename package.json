{
  "name": "clear-ai-v2",
  "version": "1.0.0",
  "description": "Modular AI agent system with LangGraph and MCP tools",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "clear-ai-mcp": "./dist/main.js"
  },
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsc && node dist/main.js",
    "api:dev": "tsc && node dist/api/server.js",
    "api:start": "node dist/api/server.js",
    "seed": "tsc && node dist/api/db/seed.js",
    "test": "jest --testPathIgnorePatterns=integration",
    "test:watch": "jest --watch --testPathIgnorePatterns=integration",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=integration",
    "test:integration": "jest integration --testTimeout=60000",
    "test:all": "jest --testTimeout=60000",
    "test:esm": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:llm": "tsc && node dist/scripts/test-llm-providers.js",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agent",
    "langgraph",
    "mcp",
    "tools",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.4.9",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "langfuse": "^3.38.5",
    "mongoose": "^8.0.0",
    "neo4j-driver": "^6.0.0",
    "openai": "^6.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiktoken": "^1.0.22",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.7.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "jest-util": "^30.2.0",
    "nock": "^14.0.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5"
  }
}
